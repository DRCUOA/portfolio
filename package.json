{
  "name": "portfolio",
  "version": "1.0.1",
  "scripts": {
    "dev": "concurrently -n \"API,FRONTEND\" -c \"cyan,magenta\" --kill-others-on-fail --kill-others \"npm run dev:api\" \"npm run dev:frontend\"",
    "dev:api": "cd api && npm run dev:api",
    "dev:frontend": "cd frontend && npm run dev"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
